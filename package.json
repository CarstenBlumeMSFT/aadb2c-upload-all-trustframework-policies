{
  "name": "aadb2c-upload-all-trustframework-policies",
  "version": "0.0.1",
  "private": true,
  "description": "A GitHub Action for uploading all Azure AD B2C policies from a selected folder as generated by the VSCode extension",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlowerFYI/aadb2c-upload-all-trustframework-policies.git"
  },
  "keywords": [
    "actions",
    "AAD",
    "B2C",
    "custom policies",
    "trustframework"
  ],
  "author": "Carsten Blume",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "aadb2c-core-modules-js": "github:FlowerFYI/aadb2c-core-modules-js"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.3",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
